<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.goka.favos" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ui.PhotoListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.PhotoDetailActivity"
            android:label="@string/title_photo_detail"
            android:parentActivityName=".ui.PhotoListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="jp.goka.favos.PhotoListActivity" />
        </activity>
    </application>

</manifest>
